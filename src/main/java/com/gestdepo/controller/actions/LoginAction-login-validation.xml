<!DOCTYPE validators PUBLIC
"-//OpenSymphony Group//XWork Validator 1.0//EN"
"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
    <field name="userName">
        <field-validator type="requiredstring">
            <message key="login.validation.user.name" />
        </field-validator>
    </field>
    <field name="userPwd">
        <field-validator type="requiredstring">
            <message key="login.validation.password" />
        </field-validator>
    </field>
</validators>